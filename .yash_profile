# Load in default profile...
if [ -f "$HOME/.profile" ]; then
  # shellcheck source=/dev/null
  . "$HOME/.profile"
else
  echo ".profile not found, environment maybe unstable" 2>&1
fi

# Load in .yashrc...
if [ -f "$HOME/.yashrc" ]; then
  # shellcheck source=/dev/null
  . "$HOME/.yashrc"
else
  echo ".yashrc not found, environment maybe unstable" 2>&1
fi
